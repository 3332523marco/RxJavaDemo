<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:orientation="vertical"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">
    <Button
       android:id="@+id/testMap"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="testMap"/>

    <Button
        android:id="@+id/testFilter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testFilter"/>

    <Button
        android:id="@+id/testThread"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testThread"/>
    <Button
        android:id="@+id/testDefer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testDefer"/>
    <Button
        android:id="@+id/testRetrofit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testRetrofit"/>
</LinearLayout>
